<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <LangVersion>11.0</LangVersion>
    <TargetFrameworks>net8.0</TargetFrameworks>
	  <Nullable>annotations</Nullable>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <PropertyGroup>
    <NoWarn>$(NoWarn);NU1605;CS1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Graphics\DefaultGraphicEngine.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
    <None Include="..\resources\logo\nuget-logo.png" Pack="true" PackagePath="\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="3.1.0" />
    <PackageReference Include="ExcelNumberFormat" Version="1.1.0" />
    <!--PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" /-->
    <PackageReference Include="RBush" Version="3.2.0" />
    <PackageReference Include="ClosedXML.Parser" Version="[1.2.0,2.0.0)" />
    <PackageReference Include="System.IO.Packaging" Version="8.0.0" />
  </ItemGroup>
</Project>